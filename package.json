{
  "name": "twitch-chess",
  "version": "1.0.0",
  "description": "Web based chess that allows twitch audience to play with the streamer",
  "author": "Taylor Dondich <taylor@dondich.com>",
  "private": true,
  "scripts": {
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "webpack",
    "dev": "node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "install": "find ./node_modules/* -mtime +10950 -exec touch {} \\;"
  },
  "dependencies": {
    "chess-rules": "^1.1.0",
    "colors": "^1.2.5",
    "cross-env": "^5.1.5",
    "ejs": "^2.6.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "mysql2": "^1.5.3",
    "popper.js": "^1.14.3",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "sequelize": "^4.37.7",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.0",
    "twitch-bot": "^1.2.5",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "websocket": "^1.0.26"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.11",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
